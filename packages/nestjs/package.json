{
  "name": "@tusss/nestjs",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && swc src -d dist --strip-leading-paths && pnpm run build:types",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "check:types": "tsc --noEmit"
  },
  "private": true,
  "type": "commonjs",
  "packageManager": "pnpm@10.23.0",
  "devDependencies": {
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.3",
    "@tusss/eslint": "workspace:*",
    "@tusss/tsconfig": "workspace:*",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@tusss/core": "workspace:*"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./swagger": {
      "types": "./dist/swagger/index.d.ts",
      "require": "./dist/swagger/index.js",
      "import": "./dist/swagger/index.js"
    }
  }
}
